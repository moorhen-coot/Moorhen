#!/bin/sh

if command -v greadlink > /dev/null 2>&1; then
    MOORHEN_SOURCE_DIR=`dirname -- "$( greadlink -f -- "$0"; )"`
else
    MOORHEN_SOURCE_DIR=`dirname -- "$( readlink -f -- "$0"; )"`
fi

. ${MOORHEN_SOURCE_DIR}/get_sources_funs

geteigen
getlibccp4
getclipper
getssm
getmmdb2
getgemmi
getmonomers
getjsoncpp
getigraph
getslicendice
getfftw
getgsl
getcoot
getglm
getrdkit
getgraphene
getsigcpp
getprivateer
getboost
