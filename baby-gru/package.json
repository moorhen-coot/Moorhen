{
    "name": "moorhen",
    "version": "0.23.0-dev",
    "private": false,
    "main": "moorhen.js",
    "types": "moorhen.d.ts",
    "homepage": "./",
    "author": "Paul Emsley, Filomeno Sanchez Rodriguez, Martin Noble, Jordan Dialpuri, Jakub Smulski, Lucrezia Catapano, Paul Bond, Clément Dégut, Stuart McNicholas",
    "description": "Moorhen molecular graphics",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moorhen-coot/Moorhen.git"
    },
    "bugs": {
        "url": "https://github.com/moorhen-coot/Moorhen/issues"
    },
    "keywords": [
        "molecular graphics",
        "moorhen",
        "webcoot",
        "ccp4",
        "interactive model building"
    ],
    "scripts": {
        "transpile-graphql-codegen": "if ! test -r src/utils/__graphql__/gql.ts; then  npx graphql-codegen; fi",
        "transpile-protobuf": "npx pbjs -t static-module -w es6 -o ./src/protobuf/MoorhenSession.js ./src/protobuf/MoorhenSession.proto",
        "create-version": "npx genversion -e -p ./package.json -d -s ./src/version.js",
        "transpile-ts-worker": "npx tsc --module es2015 --lib webworker --skipLibCheck public/CootWorker.ts && sed -i.bak '/export {}/d' public/CootWorker.js && rm public/CootWorker.js.bak",
        "transpile-ts-worker-jest": "npx tsc --module es2015 --lib webworker --skipLibCheck public/CootWorker.ts && sed -i.bak 's/export {}/export { doCootCommand, setModules }/g' public/CootWorker.js && sed -i.bak '/importScripts.*;/d' public/CootWorker.js && sed -i.bak 's/onmessage/function setModules(arg0, arg1) { molecules_container = arg0; cootModule = arg1 }; let postMessage = () => {}; let onmessage/g' public/CootWorker.js && rm public/CootWorker.js.bak",
        "prestart": "npm run create-version && npm run transpile-ts-worker && npm run transpile-protobuf && npm run transpile-graphql-codegen",
        "start": "vite --config vite.config.mts",
        "prebuild": "npm run create-version && npm run transpile-ts-worker && npm run transpile-protobuf && npm run transpile-graphql-codegen",
        "build": "vite build",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "dts-bundle": "npx dts-bundle-generator -o dist/moorhen.d.ts src/moorhen.ts --no-check",
        "build-release": "npm run prebuild && webpack --mode production --devtool inline-source-map && npm run dts-bundle && ./fix_old_namespace.sh",
        "build-release-dev": "npm run prebuild && webpack --mode development --devtool inline-source-map --no-optimization-minimize --no-optimization-concatenate-modules && npm run dts-bundle && ./fix_old_namespace.sh",
        "create-docs": "npm run create-version && npx jsdoc -c jsdoc.json ../README.md && mkdir -p docs/wasm_src_frontend && cp -a ../wasm_src_frontend/baby_gru.png docs/wasm_src_frontend",
        "test": "npm run create-version && npm run transpile-protobuf && npm run transpile-graphql-codegen && npx tsc && npm run transpile-ts-worker-jest && ln -sf ./public/ ./baby-gru && node --trace-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
        "test-react": "npm run create-version && npm run transpile-protobuf && npm run transpile-graphql-codegen && npx tsc && node --trace-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js --detectOpenHandles --forceExit --testPathIgnorePatterns=MoorhenContainer.test.jsx --testPathIgnorePatterns=MoorhenLigandMenu.test.jsx --testPathIgnorePatterns=MoorhenEditMenu.test.jsx --selectProjects react-components"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "peerDependencies": {
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "@reduxjs/toolkit": "^2.9.0",
        "react-redux": "^9.2.0"
    },
    "dependencies": {
        "@apollo/client": "^3.13.8",
        "@babel/core": "^7.28.4",
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-react": "^7.27.1",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@graphql-codegen/cli": "^6.0.0",
        "@graphql-codegen/client-preset": "^5.1.0",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@mui/icons-material": "^7.3.4",
        "@mui/lab": "^7.0.1-beta.18",
        "@mui/material": "^7.3.4",
        "@mui/x-tree-view": "^8.14.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.7.1",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.1",
        "@vitejs/plugin-react-swc": "^4.1.0",
        "babel-loader": "^10.0.0",
        "babel-preset-react-app": "^10.1.0",
        "biojs-io-fasta": "^0.1.9",
        "bootstrap": "^5.3.8",
        "chart.js": "^4.5.0",
        "chartjs-plugin-annotation": "^3.1.0",
        "concurrently": "^9.2.1",
        "copy-webpack-plugin": "^13.0.1",
        "css-loader": "^7.1.2",
        "d3": "^7.9.0",
        "file-loader": "^6.2.0",
        "genversion": "^3.2.0",
        "gl-matrix": "^3.4.4",
        "graphql": "^16.11.0",
        "html-react-parser": "^5.2.6",
        "iris-validation": "^0.1.10",
        "iris-validation-backend": "^0.0.8",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "jsdoc": "^4.0.5",
        "localforage": "^1.10.0",
        "mini-css-extract-plugin": "^2.9.4",
        "nested-worker": "git+https://github.com/johanholmerin/nested-worker.git#semver:^1.0.0",
        "node-fetch": "^3.3.2",
        "node-gzip": "^1.1.2",
        "nodemon": "^3.1.10",
        "notistack": "^3.0.2",
        "pako": "^2.1.0",
        "prismjs": "^1.30.0",
        "protobufjs": "^7.5.4",
        "protobufjs-cli": "^1.1.3",
        "react-bootstrap": "^2.10.10",
        "react-bootstrap-typeahead": "^6.4.1",
        "react-colorful": "^5.6.1",
        "react-draggable": "^4.5.0",
        "react-dropzone": "^14.3.8",
        "react-hotkeys": "^2.0.0",
        "react-resizable": "^3.0.5",
        "react-scan": "^0.4.3",
        "react-simple-code-editor": "^0.14.1",
        "sass": "^1.93.2",
        "sass-loader": "^16.0.5",
        "style-loader": "^4.0.0",
        "styled-components": "^6.1.19",
        "ts-jest": "^29.4.5",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "util": "^0.12.5",
        "uuid": "^13.0.0",
        "vite": "^7.1.9",
        "vite-plugin-checker": "^0.11.0",
        "vite-plugin-cross-origin-isolation": "^0.1.6",
        "vite-plugin-top-level-await": "^1.6.0",
        "vite-plugin-wasm": "^3.5.0",
        "vite-pwa": "^1.0.2",
        "wait-on": "^9.0.1",
        "web-vitals": "^5.1.0",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-node-externals": "^3.0.0",
        "worker-loader": "^3.0.8",
        "y-websocket": "^3.0.0",
        "yjs": "^13.6.27"
    },
    "devDependencies": {
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-transform-object-rest-spread": "^7.28.4",
        "@eslint/css": "^0.12.0",
        "@eslint/js": "^9.37.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@typescript-eslint/eslint-plugin": "^8.46.0",
        "@typescript-eslint/parser": "^8.46.0",
        "@vitejs/plugin-react": "^5.0.4",
        "babel-plugin-react-compiler": "^19.1.0-rc.2",
        "docdash": "^2.0.2",
        "dts-bundle-generator": "^9.5.1",
        "eslint": "^9.37.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.0",
        "eslint-plugin-react-hooks-extra": "^2.0.6",
        "eslint-plugin-react-perf": "^3.3.3",
        "eslint-plugin-react-refresh": "^0.4.23",
        "eslint-plugin-react-you-might-not-need-an-effect": "^0.5.6",
        "globals": "^16.4.0",
        "jsdoc-babel": "^0.5.0",
        "react-compiler-webpack": "^0.2.1",
        "vite-plugin-babel": "^1.3.2",
        "vite-plugin-pwa": "^1.0.3",
        "vite-plugin-react": "^4.0.1"
    }
}
