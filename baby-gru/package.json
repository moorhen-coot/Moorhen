{
  "name": "Moorhen",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": "Paul Emsley, Filomeno Sanchez Rodriguez, Martin Noble, Stuart McNicholas",
  "description": "Moorhen molecular graphics",
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "icon": "src/icons/icon",
        "name": "Moorhen"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Moorhen"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin","linux"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.1",
    "chartjs-plugin-annotation": "^2.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "d3": "^5.15.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "gl-matrix": "^3.4.3",
    "html-react-parser": "^1.4.14",
    "localforage": "^1.10.0",
    "nested-worker": "git+https://github.com/johanholmerin/nested-worker.git#semver:^1.0.0",
    "pako": "^2.0.4",
    "pca-js": "^1.0.2",
    "plotly.js": "^2.12.1",
    "prop-types": "^15.8.1",
    "protvista-manager": "^3.4.0",
    "protvista-navigation": "^3.5.1",
    "protvista-sequence": "^3.5.1",
    "protvista-track": "^3.4.0",
    "rc-tree": "4.2.2",
    "react": "^18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-bootstrap-typeahead": "^6.0.0-alpha.11",
    "react-chartjs-2": "^4.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.1.0",
    "react-plotly.js": "^2.5.1",
    "react-scripts": "5.0.1",
    "reactcss": "^1.2.3",
    "styled-components": "^5.3.6",
    "util": "^0.12.5",
    "uuid": "^9.0.0",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-cloud": "webpack --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make --platform darwin && electron-forge make --platform linux",
    "make-win32": "react-scripts build && electron-forge make --platform win32"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@types/node": "^18.11.9",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "electron": "^23.1.3",
    "electron-devtools-installer": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  }
}
